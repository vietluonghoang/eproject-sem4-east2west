<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>CustomerServlet</servlet-name>
        <servlet-class>e2w.servlet.CustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginCustomerServlet</servlet-name>
        <servlet-class>e2w.servlet.loginCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchTourServlet</servlet-name>
        <servlet-class>e2w.servlet.searchTourServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reportServlet</servlet-name>
        <servlet-class>e2w.servlet.reportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>flightServlet</servlet-name>
        <servlet-class>e2w.servlet.flightServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarReportServlet</servlet-name>
        <servlet-class>servlets.CarReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>TourReportServlet</servlet-name>
        <servlet-class>servlets.TourReportServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TourActionServlet</servlet-name>
        <servlet-class>servlets.TourActionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CustomerServlet</servlet-name>
        <url-pattern>/CustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginCustomerServlet</servlet-name>
        <url-pattern>/loginCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchTourServlet</servlet-name>
        <url-pattern>/searchTourServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reportServlet</servlet-name>
        <url-pattern>/reportServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>NgocServlet</servlet-name>
        <servlet-class>ngoc.servlet.NgocServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NgocServlet</servlet-name>
        <url-pattern>/NgocServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>flightServlet</servlet-name>
        <url-pattern>/flightServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ActionServlet</servlet-name>
        <servlet-class>servlets.ActionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ActionServlet</servlet-name>
        <url-pattern>/ActionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarReportServlet</servlet-name>
        <url-pattern>/CarReportServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TourReportServlet</servlet-name>
        <url-pattern>/TourReportServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TourActionServlet</servlet-name>
        <url-pattern>/TourActionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/Ehome.jsp</welcome-file>
    </welcome-file-list>
</web-app>
